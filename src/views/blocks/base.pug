block vars
	- var $title = undefined
	- var $script = undefined
	- var $script = undefined
	- var $lang = "ru"

doctype html
html(lang=$lang)
	head
		meta(charset="UTF-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		title= title
		if $style
			link(rel="stylesheet", href=$style)
		include @utils/_favicons.html
	body
		block body
		if $script
			script(src=$script)
